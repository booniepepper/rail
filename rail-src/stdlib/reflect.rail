# Define "string?"
[ type "string" eq? ] [ string? ] def

# Define its friends
[ "i64" "f64" "command" "quote" "bool" "stab" ]
[ [ name ] ->
    [ type ] name push [ eq? ] concat  # [ type "i64" eq? ]
    name [ "?" ] enq "" join           # "i64?"
    quote enq
] map
[ [ def ] concat do! ] each!

[ unquote ] [ ... ] def
